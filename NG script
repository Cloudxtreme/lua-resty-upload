--------STEP[1] Lobby👇--------

function BP() --📂 Menu Lobby
ML = gg.multiChoice({
           "🔥Bypass",
           "Name",
           "Name",
           "Name",
           "Name",
           "BACK"
}, nil, "Type Information:-")
  if ML == nil then
  else
    if ML [1] == true then
      BP()
    end
    if ML [2] == true then
     LR()
    end
    if ML [3] == true then
     ML3()
    end
    if ML [4] == true then
     ML4()
    end
    if ML [5] == true then
      ML5()
    end
    if ML [6] == true then
      HOME()
    end
  end
  PUBGMH = -1
end

