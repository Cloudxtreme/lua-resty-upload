function HOME() --Home Menu
MENU = gg.choice({
	       "ğŸ”¥Bypass (Lobby)",
	       "ğŸ”¥Wallhach & Color (Training)",
           "ğŸ”¥Less recoil(Lobby)",
           "ğŸ”¥Headshot Magic (Game)",
           "ğŸ”¥Damage M416 (Game)",
           "ğŸ”¥Kar98k brutal shot (Game)",
           "ğŸ”¥Tabe View (Game)",
           "ğŸ‘‰ E X I T ğŸ‘ˆ"
}, nil, "ğŸ‡²ğŸ‡² ğğ† ğ’ğ‚ğ‘ğˆğğ“ ğ’16 ğŸ‡²ğŸ‡² \n âƒâƒâƒâƒâƒâƒâƒâƒâƒâƒâƒâƒâƒâƒâƒ")
    if MENU == nil then
  else
    if MENU == 1 then
      BP()
    end
    if MENU == 2 then
     WH()
    end
    if MENU == 3 then
     LR()
    end
    if MENU == 4 then
     HS()
     end
     if MENU == 5 then
      DM()
      end
      if MENU == 6 then
       KR()
       end
       if MENU == 7 then
        TV()
        end      
    if MENU == 8 then
      EXIT()
    end
  end
  PUBGMH = -1
end



--------STEP[2] Wall & ColourğŸ‘‡--------

function WH() --Wall & Colour Menu
WC = gg.multiChoice({
           "ğŸ’¥Wallhack SD 625",
           "ğŸ’¥Wallhack SD 636",
           "ğŸ’¥wallhack SD 660",
           "ğŸ’¥Yellow",
           "ğŸ’¥Read",
           "ğŸ‘‰BACKğŸ‘ˆ"
}, nil, "Type Information")
if WC == nil then
  else
    if WC [1] == true then
      SD625()
    end
    if WC [2] == true then
     SD636()
    end
    if WC [3] == true then
     SD660()
    end
    if WC [4] == true then
     YEL()
    end
    if WC [5] == true then
      RED()
    end
    if WC [6] == true then
      HOME()
    end
  end
  PUBGMH = -1
end

function SD625() 
gg.clearResults()
gg.setRanges(gg.REGION_VIDEO or gg.REGION_BAD)
gg.searchNumber("1.8948778e-40;4.7408166e21;2.0:93", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.searchNumber("2", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.refineAddress("504", -1, gg.TYPE_FLOAT, gg.SIGN_EQUAL, 0, -1)
whs625s1 = gg.getResults(100)
gg.editAll("120", gg.TYPE_FLOAT)
gg.clearResults()
gg.setRanges(gg.REGION_VIDEO or gg.REGION_BAD)
gg.searchNumber("3.37670946121;3.37548875809;2.0:149", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.searchNumber("2", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.refineAddress("980", -1, gg.TYPE_FLOAT, gg.SIGN_EQUAL, 0, -1)
whs625s2 = gg.getResults(100)
gg.editAll("120", gg.TYPE_FLOAT)
gg.clearResults()
gg.setRanges(gg.REGION_VIDEO or gg.REGION_BAD)
gg.searchNumber("2", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.searchNumber("2", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
gg.refineAddress("900", -1, gg.TYPE_FLOAT, gg.SIGN_EQUAL, 0, -1)
whs625s3 = gg.getResults(100)
gg.editAll("120", gg.TYPE_FLOAT)
gg.clearResults()
gg.toast("WALLHACK Activated")
end
 
